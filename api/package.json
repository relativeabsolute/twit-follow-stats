{
    "name": "api",
    "version": "0.1.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "start": "npm run run-server",
        "start-local": "npm run build && npm run run-server",
        "build": "npm run clean && concurrently \"npm run build-static-files\" \"npm run build-server\" && npm run move-static-files",
        "build-server": "tsc",
        "clean": "rimraf ./dist",
        "run-server": "node ./dist/index.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build-static-files": "cd ../frontend && npm run build:staging && cd ../api",
        "move-static-files": "mv ../frontend/dist ./dist/public",
        "lint": "tslint src/**/*.ts",
        "build-static-files:prod": "cd ../frontend && npm run build:prod && cd ../api"
    },
    "keywords": [],
    "author": "Johan Burke",
    "license": "MIT",
    "dependencies": {
        "@reactivex/rxjs": "^6.6.3",
        "bent": "^7.3.12",
        "concurrently": "^5.3.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "rxjs": "^6.6.3",
        "underscore": "^1.11.0"
    },
    "devDependencies": {
        "@types/bent": "^7.3.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.9",
        "@types/node": "^14.14.7",
        "@types/underscore": "^1.10.24",
        "codelyzer": "^6.0.1",
        "rimraf": "^3.0.2",
        "tslint": "^6.1.3",
        "typescript": "^4.0.5"
    }
}
